{
  "name": "v2p2-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "https-preview": "serve dist",
    "cypress": "cypress run",
